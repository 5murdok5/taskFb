{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "./node_modules/standard/eslintrc.json",
        "eslint:recomended",
        "plugin:react/recommended",
        "plugin:prettier/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "html"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/prop-types": 0
    }
}
